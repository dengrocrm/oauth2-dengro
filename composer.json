{
    "name": "dengrocrm/oauth2-dengro",
    "description": "DenGro OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Davidson",
            "email": "dan@dengro.com",
            "homepage": "https://github.com/dengrocrm"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation",
        "dengro"
    ],
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Dengro\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dengro\\OAuth2\\Client\\Test\\": "test/src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
